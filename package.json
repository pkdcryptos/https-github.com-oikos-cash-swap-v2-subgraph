{
  "name": "@oikos/swap-v2-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/oikos-cash/swap-v2-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "codegen": "graph codegen --output-dir src/types/",
    "build": "graph build",
    "create:local": "graph create oikos/swapv2 --node http://127.0.0.1:8020",
    "deploy:local": "graph deploy oikos/swapv2 --debug --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "remove:local": "graph remove oikos/swapv2 --node http://127.0.0.1:8020",
    "create:mainnet": "graph create oikos/swapv2 --node  https://thegraph-admin.oikos.cash --debug",
    "deploy:mainnet": "graph deploy oikos/swapv2 --ipfs http://ipfs.oikos.cash --node https://thegraph-admin.oikos.cash --debug",
    "remove:mainnet": "graph remove oikos/swapv2 --node https://thegraph-admin.oikos.cash"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
